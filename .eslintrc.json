{
 "extends": ["eslint:recommended", "plugin:react/recommended"],
 "plugins": ["react", "react-hooks"],
 "rules": {
  "react/jsx-no-undef": "error",
  "no-undef": "error",
  "react/prop-types": "off"
 },
 "env": {
  "browser": true,
  "node": true,
  "es2021": true
 },
 "settings": {
  "react": {
   "version": "detect"
  }
 },
 "parserOptions": {
  "ecmaVersion": "latest",
  "sourceType": "module"
 },
 "ignorePatterns": ["**/*.scss", "**/*.sass", "**/*.css", "**/*.coffee"]
}
